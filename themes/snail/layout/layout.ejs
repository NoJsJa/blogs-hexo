<!DOCTYPE html>
<html lang="en">

<!-- Head tag -->
<%- partial('_partial/head') %>

<!-- hack iOS CSS :active style -->
<body ontouchstart="">

    <!-- background effects start -->
    <% if(config['color_theme'] == 'star'){ %>
        <%- partial('_partial/star.ejs') %>
    <% } %>
    <!-- background effects end -->

	<%- partial('_partial/header',{cache: true}) %>
	
    <%- partial('_partial/nav',{cache: true}) %>

    <!-- Main Content -->
    <%- body %>

    <!-- Footer -->
    <%- partial('_partial/footer') %>


	<a id="rocket" href="#top" class=""></a>
	<script type="text/javascript" src="/blogs/js/totop.js" async=""></script>
    <script type="text/javascript" src="/blogs/js/toc.js" async=""></script>

    <% if(config['bg_effects']['enable']){ %>
        <!-- background effects line -->
        <% if(config['bg_effects']['line']['enable']){ %>
            <!--<script type="text/javascript" color="<%= config.bg_effects.color %>" pointColor="<%= config.bg_effects.pointColor %>" opacity='<%= config.bg_effects.opacity %>' zIndex="<%= config.bg_effects.zIndex %>" count="<%= config.bg_effects.count %>" src="/js/canvas-nest.js"></script>-->
            <script type="text/javascript" src="/blogs/js/line.js"></script>
        <% } %>

        <% if(config['bg_effects']['mouse_click']['enable']){ %>
            <script type="text/javascript" src="/blogs/js/mouse-click.js" content='[<%= config.bg_effects.mouse_click.content %>]' color='[<%= config.bg_effects.mouse_click.color %>]'></script>
        <% } %>

        <!-- background effects end -->
    <% } %>

    <!--<script size="50" alpha='0.3' zIndex="-999" src="/js/ribbonStatic.js"></script>-->
    <% if(config['color_theme'] != 'star'){ %>
        <script src="/blogs/js/ribbonDynamic.js"></script>
    <% } %>
</body>

</html>
